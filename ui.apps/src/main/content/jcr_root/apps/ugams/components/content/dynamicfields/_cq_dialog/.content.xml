<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="DynamicFields"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        jcr:title="DynamicFields"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="DynamicFields"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <fieldsname
                                        granite:class="cq-dialog-dropdown-showhide"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Type"
                                        name="./fieldsName">
                                        <items jcr:primaryType="nt:unstructured">
                                            <phoneno
                                                jcr:primaryType="nt:unstructured"
                                                text="Phone No"
                                                value="phoneno"/>
                                            <address
                                                jcr:primaryType="nt:unstructured"
                                                text="Address"
                                                value="address"/>
                                            <gender
                                                jcr:primaryType="nt:unstructured"
                                                text="Gender"
                                                value="gender"/>
                                            <languages
                                                jcr:primaryType="nt:unstructured"
                                                text="Languages"
                                                value="languages"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            cq-dialog-dropdown-showhide-target=".fieldType-showhide-target"/>
                                    </fieldsname>
                                    <phoneno
                                        granite:class="fieldType-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <contactno
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Contact No"
                                                name="./contact"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="phoneno"/>
                                    </phoneno>
                                    <address
                                        granite:class="fieldType-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <street
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Street"
                                                name="./street"/>
                                            <city
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City"
                                                name="./city"/>
                                            <state
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="State"
                                                name="./state"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="address"/>
                                    </address>
                                    <gender
                                        granite:class="fieldType-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <radiogroup
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                name="./selectedgender">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <male
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Male"
                                                        value="Male"/>
                                                    <female
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Female"
                                                        value="Female"/>
                                                </items>
                                            </radiogroup>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="gender"/>
                                    </gender>
                                    <languages
                                        granite:class="fieldType-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <english
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./english"
                                                text="English"
                                                value="english"/>
                                            <hindi
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./hindi"
                                                text="Hindi"
                                                value="hindi"/>
                                            <marathi
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./marathi"
                                                text="Marathi"
                                                value="marathi"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="languages"/>
                                    </languages>
                                </items>
                            </column>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
